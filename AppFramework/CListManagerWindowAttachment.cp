#include "CListManagerWindowAttachment.h"#include "CWindow.h"#include <string.h>CListManagerWindowAttachment::CListManagerWindowAttachment(CWindow *wd, Rect *box): CWindowAttachment(wd) {	const char* str = NULL;	Rect bodyBox = *box;	bodyBox.right -= 15; // Account for scroll bar.	Rect dataBounds = { 0, 0, 3, 1};	Point cellSize = { 0, 0 };	Point cell = { 0, 0 };	mList = LNew(&bodyBox, &dataBounds, cellSize, 0, wd->ToMac(), true, false, false, true);		str = "First Row";	LSetCell(str, strlen(str), cell, mList);	cell.v += 1;	str = "Second Row";	LSetCell(str, strlen(str), cell, mList);	cell.v += 1;	str = "Third Row";	LSetCell(str, strlen(str), cell, mList);}CListManagerWindowAttachment::~CListManagerWindowAttachment() {	LDispose(mList);	mList = NULL;}void CListManagerWindowAttachment::Draw() {	ForeColor(whiteColor);	PaintRect(&(**mList).rView);		ForeColor(blackColor);	LUpdate((**mList).port->visRgn, mList);		FrameRect(&(**mList).rView);}	void CListManagerWindowAttachment::ResizedWindowFrom(Point oldSize) {	Rect box = (**mList).rView;	ResizedFromOldToNewWindowRect(&box, oldSize, &(**mList).port->portRect);	LSize(box.right - box.left, box.bottom - box.top, mList);	(**mList).rView = box;}	void CListManagerWindowAttachment::Activate() {}void CListManagerWindowAttachment::Deactivate() {}