#include "CFilieWindow.h"#include "CScrollbarControl.h"#include "CButtonControl.h"#include "CListManagerWindowAttachment.h"void CFilieWindow::CreateWindow() {	CWindow::CreateWindow();	Rect box = mWindow->portRect;	Rect scrollerRect = { 0, 0, box.bottom - box.top, box.right - box.left };	scrollerRect.left = scrollerRect.right - 15;	scrollerRect.right += 1; // "tuck under" the window border.	scrollerRect.bottom -= 15;	scrollerRect.bottom += 1; // "tuck under" the window border.	scrollerRect.top -= 1; // "tuck under" the window border.	CControl* control = new CScrollbarControl(this, &scrollerRect, 50, 0, 100, 'hscr');	control->SetAutoPositionFlags(kAutoPositionFlagPinRight | kAutoPositionFlagResizeVert);		SetRect(&scrollerRect, 0, 0, box.right - box.left, box.bottom - box.top);	scrollerRect.left -= 1; // "tuck under" the window border.	scrollerRect.right -= 15;	scrollerRect.right += 1; // "tuck under" the window border.	scrollerRect.top = scrollerRect.bottom - 15;	scrollerRect.bottom += 1; // "tuck under" the window border.	control = new CScrollbarControl(this, &scrollerRect, 50, 0, 100, 'vscr');	control->SetAutoPositionFlags(kAutoPositionFlagPinBottom | kAutoPositionFlagResizeHorz);	Rect buttonRect;	SetRect(&buttonRect, 0, 0, box.right - box.left, box.bottom - box.top);	buttonRect.bottom = scrollerRect.top - 20;	buttonRect.top = buttonRect.bottom - 22;	buttonRect.right = scrollerRect.right - 20;	buttonRect.left = buttonRect.right - 88;	control = new CButtonControl(this, &buttonRect, "\pOK", 'clos');	//control = new CControl(this, &buttonRect, "\pOK", 50, 0, 100, 11200);	control->SetAutoPositionFlags(kAutoPositionFlagPinBottom | kAutoPositionFlagPinRight);		Rect listRect;	SetRect(&listRect, 0, 0, box.right - box.left, box.bottom - box.top);	InsetRect(&listRect, 20, 20);	listRect.bottom = buttonRect.top -20;	CListManagerWindowAttachment * att = new CListManagerWindowAttachment(this, &listRect);}